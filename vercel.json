{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "functions": {
    "api/contact.js": {
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://aiflows.pro/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "n8npro.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "https://aiflows.pro/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.n8npro.com"
        }
      ]
    }
  ]
} 